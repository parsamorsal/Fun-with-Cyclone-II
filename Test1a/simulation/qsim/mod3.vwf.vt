`timescale 1 ps/ 1 ps
module mod3_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [3:0] data;
// wires                                               
wire [1:0] out;

// assign statements (if any)                          
mod3 i1 (
// port map - connection between master ports and signals/registers   
	.data(data),
	.out(out)
);
initial 
begin 
#1000000 $finish;
end 
// data[ 3 ]
initial
begin
	repeat(6)
	begin
		data[3] = 1'b0;
		data[3] = #80000 1'b1;
		# 80000;
	end
	data[3] = 1'b0;
end 
// data[ 2 ]
initial
begin
	repeat(12)
	begin
		data[2] = 1'b0;
		data[2] = #40000 1'b1;
		# 40000;
	end
	data[2] = 1'b0;
end 
// data[ 1 ]
always
begin
	data[1] = 1'b0;
	data[1] = #20000 1'b1;
	#20000;
end 
// data[ 0 ]
always
begin
	data[0] = 1'b0;
	data[0] = #10000 1'b1;
	#10000;
end 
endmodule

